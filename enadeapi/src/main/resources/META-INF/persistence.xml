<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.2" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
  <!-- Define Persistence Unit -->
  <persistence-unit name="enade.PU">
    <class>com.gabrielrigolon.enadeapi.models.Tbprova</class>
    <class>com.gabrielrigolon.enadeapi.models.Tbtipoquestao</class>
    <class>com.gabrielrigolon.enadeapi.models.Tbresultado</class>
    <class>com.gabrielrigolon.enadeapi.models.Tbquestao</class>
    <class>com.gabrielrigolon.enadeapi.models.Tbtipousuario</class>
    <class>com.gabrielrigolon.enadeapi.models.Tbusuario</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/enade?useTimezone=true&amp;serverTimezone=UTC&amp;zeroDateTimeBehavior=CONVERT_TO_NULL"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
